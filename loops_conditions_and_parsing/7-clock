#!/usr/bin/env bash
# Initialize variables for hours and minutes
hour=0
minute=1

# Use a while loop to display the time
while [ $hour -le 12 ]; do
  while [ $minute -le 59 ]; do
    printf "%02d:%02d\n" $hour $minute
    ((minute++))
  done
  ((hour++))
  minute=1  # Reset minutes to 1 after reaching 59
done
